float temperature;
float wilgotnosc;
char temperatureString[6] = "-";
char wilgstr[6] = "-";
char wyswysokosc[10] = "-";
char wyspredkosc[10] = "-";
char wysdom[10] = "-";
char wyssatelity[10] = " ";
char wysszerokosc[20] = " ";
char wysdlugosc[20] = " ";
char wysgodziny[5] = " ";
char wysminuty[5] = " ";
char wyssekundy[5] = " ";
char wysrok[5] = " ";
char wysmiesiac[5] = " ";
char wysdzien[5] = " ";


int godziny, minuty, sekundy;
int rok, miesiac, dzien;


float R1 = 4660; // Prawdziwa rezystancja R1 (4,7K)
float R2 = 1995; // Prawdziwa rezystancja R2 (2K)

float wartosc = 0;
float vout = 0.0;
float vin = 0.0;
int analogInput = 0; // POMIAR NAPIÄ�CIA NA A0
char napieciestr[6] = "-";
int wystemp = 0;
float wysnap = 0;
int wybor = 0;
float predkosc = 0;
float dodomu = 0.0;
int wysokosc = 0;
int counter = 0;
int counter2 = 0;
int countermenu = 0;
int satelity = 0;
double szerokosc;
double dlugosc;



int zapisane = 0;
int menu = 0;
int logo = 0;

const int buttonPin = 2;
int buttonState = 0;

void setup() {
  logo = EEPROM.readInt(64);
  switch (logo)
  {
    case 0:
      u8g.firstPage();
      do {
        u8g.drawBitmapP( 0, 0, 16, 64, honda);
      } while ( u8g.nextPage() );
      break;
    case 1:
      u8g.firstPage();
      do {
        u8g.drawBitmapP( 0, 0, 16, 64, kawasaki);
      } while ( u8g.nextPage() );
      break;
    case 2:
      u8g.firstPage();
      do {
        u8g.drawBitmapP( 0, 0, 16, 64, suzuki);
      } while ( u8g.nextPage() );
      break;
    case 3:
      u8g.firstPage();
      do {
        u8g.drawBitmapP( 32, 0, 8, 64, bmw);
      } while ( u8g.nextPage() );
      break;
  }

  delay(3000);

  rtc.begin();
  dht.begin();
  pinMode(analogInput, INPUT);
  pinMode(buttonPin, INPUT_PULLUP);
  ss.begin(GPSBaud);
  DOM_LAT = EEPROM.readDouble(0);
  DOM_LON = EEPROM.readDouble(32);

  buttonState = digitalRead(buttonPin);
  if (buttonState == LOW)
  {
    wybor = 3;
  }
}

void loop() {

  counter++;
  counter2++;

  buttonState = digitalRead(buttonPin);

  switch (wybor)
  {
    case 0:
      if (counter > 10)
      {
        temperature = dht.readTemperature();
        wilgotnosc = dht.readHumidity();
        dtostrf(wilgotnosc, 3, 0, wilgstr);
        counter = 0;
      }
      if (temperature < -9.9)
      {
        dtostrf(temperature, 2, 0, temperatureString);
      }
      else
      {
        dtostrf(temperature, 2, 1, temperatureString);
      }
      wystemp = temperature / 2;

      wartosc = analogRead(analogInput);
      vout = (wartosc * 4.93) / 1024.0; //4.74 = WartoĹ›Ä‡ napiÄ™cia miÄ™dzy AREF a GND
      vin = vout / (R2 / (R1 + R2));
      dtostrf(vin, 2, 1, napieciestr);
      wysnap = (vin * 11) - (110 + (vin * 1.9));


      u8g.firstPage();
      do {
        u8g.drawLine(0, 13, 128, 13);
        u8g.drawLine(0, 50, 128, 50);
        u8g.setFont(u8g_font_5x8r);
        u8g.drawStr( 17, 47,  "CELSJUSZA");
        u8g.drawStr( 94, 47,  "WOLT");
        u8g.setFont(u8g_font_fub11);
        u8g.drawStr( 0, 11, rtc.getTimeStr());
        u8g.drawStr( 63, 11, rtc.getDateStr());
        u8g.drawStr( 0, 64,  "WILGOTN:");
        u8g.drawStr( 112, 64,  "%");
        u8g.drawStr( 83, 64,  wilgstr);
        u8g.setFont(u8g_font_fub17n);
        u8g.drawStr( 81, 37, napieciestr);    
        u8g.drawStr( 16, 37,  temperatureString);   
        u8g.drawBitmapP( 0, 16, 2, 32, termometr);
        u8g.drawBitmapP( 64, 16, 2, 32, aku05);
        
        if (temperature > 38)
        {                  
          u8g.drawLine(6, 38, 6, 19);
        }
        else if (temperature < -5)
        {  
          

        }
        else
        {         
          u8g.drawLine(6, 38, 6, 38 - wystemp);
        }
        
        if (vin > 14.7)
        {            
          u8g.drawBox(68, 41, 6, 4);
          u8g.drawBox(68, 23, 6, 15);
        }
        else if (vin < 12)
        {         
          
        }
        else
        {
          u8g.drawLine(68, 44, 68, 44 - wysnap);
          u8g.drawLine(69, 44, 69, 44 - wysnap);
          u8g.drawLine(70, 44, 70, 44 - wysnap);
          u8g.drawLine(71, 44, 71, 44 - wysnap);
          u8g.drawLine(72, 44, 72, 44 - wysnap);
          u8g.drawLine(73, 44, 73, 44 - wysnap);
          u8g.drawLine(74, 44, 74, 44 - wysnap);
          u8g.drawLine(75, 44, 75, 44 - wysnap);
        }
      } while ( u8g.nextPage() );
      if (buttonState == LOW && counter2 > 5)
      {
        wybor = 1;
        counter2 = 0;
        delay(600);
      }
      break;
    case 1:
      {

        if (gps.location.isValid() == 1)
        {

          dtostrf(gps.altitude.meters(), 7, 0, wyswysokosc);

          predkosc = gps.speed.kmph();
          if (predkosc < 3)
          {
            predkosc = 0.0;
          }
          dtostrf(predkosc, 6, 0, wyspredkosc);

          unsigned long distanceToDom =
            (unsigned long)TinyGPSPlus::distanceBetween(
              gps.location.lat(),
              gps.location.lng(),
              DOM_LAT,
              DOM_LON);
          dodomu = distanceToDom / 1000.0;
          dtostrf(dodomu, 4, 1, wysdom);
        }
        smartDelay(500);

        u8g.firstPage();
        do {
          u8g.drawLine(0, 13, 128, 13);
          u8g.drawLine(0, 50, 128, 50);
          u8g.drawBitmapP( -1, 16, 3, 32, krolik);
          u8g.drawBitmapP( 62, 16, 3, 32, gora);
          u8g.setFont(u8g_font_5x8r);
          u8g.drawStr( 30, 47,  "KM/H");
          u8g.drawStr( 89, 47,  "M N.P.M.");
          u8g.setFont(u8g_font_fub11);
          u8g.drawStr( 0, 11, rtc.getTimeStr());
          u8g.drawStr( 63, 11, rtc.getDateStr());
          u8g.drawStr( 0, 64, "DOM:");
          if (dodomu < 100)
          {
            u8g.drawStr( 55, 64, wysdom);
          }
          else
          {
            u8g.drawStr( 49, 64, wysdom);
          }
          u8g.drawStr( 103, 64, "KM");
          
          u8g.setFont(u8g_font_fub14n);
          if(wysokosc<1000)
          {
            u8g.drawStr( 89, 37, wyswysokosc);
          }
          else
          {
            u8g.drawStr( 83, 37, wyswysokosc);
          }
          
          u8g.setFont(u8g_font_fub17n);
          if (predkosc < 100)
          {
            u8g.drawStr( 28, 37, wyspredkosc);
          }
          else
          {
            u8g.drawStr( 24, 37, wyspredkosc);
          }

        } while ( u8g.nextPage() );
        if (buttonState == LOW && counter2 > 5)
        {
          wybor = 0;
          counter2 = 0;
        }

        break;
      case 2:
        if (gps.location.isValid() == 1)
        {
          dtostrf(gps.satellites.value(), 5, 0, wyssatelity);

          dtostrf( gps.location.lat(), 11, 6, wysszerokosc);

          dtostrf(gps.location.lng(), 12, 6, wysdlugosc);
        }
        smartDelay(500);


        u8g.firstPage();
        do {       
          u8g.setFont(u8g_font_5x8r);
          u8g.drawStr( 7, 6, "USTAWIANIE POZYCJI DOMU");
          u8g.drawStr( 12, 15, "Liczba satelit GPS:");
          u8g.drawStr( 90, 15, wyssatelity);
          u8g.drawStr( 6, 25, "Twoja aktualna pozycja:");
          u8g.drawStr( 0, 35, "SZ:");
          u8g.drawStr( 7, 35, wysszerokosc);
          u8g.drawStr( 66, 35, "DL:");
          u8g.drawStr( 68, 35, wysdlugosc);
          if (zapisane == 0)
          {
            u8g.drawStr( 4, 48, "ABY ZAPISAC NOWA POZYCJE");
            u8g.drawStr( 2, 56, "DOMU PRZYTRZYMAJ PRZYCISK");
            u8g.drawStr( 27, 64, "PRZEZ 3 SEKUNDY");
          }
          else
          {
            u8g.drawStr( 10, 53, "ZAPISONO NOWA POZYCJE!");
          }
        } while ( u8g.nextPage() );
        if (buttonState == LOW && counter2 > 5)
        {
          delay(1900);
          if (buttonState == LOW)
          {
            szerokosc = gps.location.lat();
            dlugosc = gps.location.lng();
            EEPROM.updateDouble(0, szerokosc);
            EEPROM.updateDouble(32, dlugosc);
            zapisane = 1;
          }
        }
        break;
      case 3:
        u8g.firstPage();
        do {
          u8g.setFont(u8g_font_fub11);
          u8g.drawStr( 12, 11, "USTAWIENIA");
          u8g.setFont(u8g_font_5x8r);
          u8g.drawStr( 24, 24, "1. Pozycja domu");
          u8g.drawStr( 20, 38, "2. Data i godzina");
          u8g.drawStr( 22, 52, "3. Logo startowe");
          u8g.drawStr( 40, 63, "2019 V1.0");
          if (menu == 0)
          {
            u8g.drawStr( 102, 24, "<--");
          }
          else if (menu == 1)
          {
            u8g.drawStr( 109, 38, "<--");
          }
          else
          {
            u8g.drawStr( 104, 52, "<--");
          }
        } while ( u8g.nextPage() );
        if (buttonState == LOW && counter2 > 5)
        {
          counter2 = 0;
          if (menu == 0)
          {
            menu = 1;
            countermenu = 0;
          }
          else if (menu == 1)
          {
            menu = 2;
            countermenu = 0;
          }
          else
          {
            menu = 0;
            countermenu = 0;
          }

        }
        countermenu++;
        if (countermenu > 50)
        {
          if (menu == 0)
          {
            wybor = 2;
          }
          else if (menu == 1)
          {
            wybor = 4;
          }
          else
          {
            wybor = 5;
          }
        }

        break;
      case 4:
        smartDelay(900);

        godziny = gps.time.hour() + 2;
        if (godziny > 23) {
          godziny = godziny - 24;
        }

        minuty = gps.time.minute();
        sekundy = gps.time.second();

        rok = gps.date.year();
        miesiac = gps.date.month();
        dzien = gps.date.day();

        dtostrf(gps.satellites.value(), 5, 0, wyssatelity);
        dtostrf(godziny, 2, 0, wysgodziny);
        dtostrf(minuty, 2, 0, wysminuty);
        dtostrf(sekundy, 2, 0, wyssekundy);
        dtostrf(rok, 4, 0, wysrok);
        dtostrf(miesiac, 2, 0, wysmiesiac);
        dtostrf(dzien, 2, 0, wysdzien);
        u8g.firstPage();
        do {
          u8g.setFont(u8g_font_5x8r);
          u8g.drawStr(1, 6, "USTAWIENIA DATY I GODZINY");
          u8g.drawStr( 12, 14, "Liczba satelit GPS:");
          u8g.drawStr( 90, 14, wyssatelity);
          u8g.drawStr( 20, 26, "Godzina:");
          u8g.drawStr( 62, 26, wysgodziny);
          u8g.drawStr( 72, 26, ":");
          u8g.drawStr( 77, 26, wysminuty);
          u8g.drawStr( 87, 26, ":");
          u8g.drawStr( 92, 26, wyssekundy);
          u8g.drawStr( 22, 34, "Data:");
          u8g.drawStr( 50, 34, wysdzien);
          u8g.drawStr( 60, 34, "/");
          u8g.drawStr( 65, 34, wysmiesiac);
          u8g.drawStr( 75, 34, "/");
          u8g.drawStr( 80, 34, wysrok);
          if (zapisane == 0)
          {
            u8g.drawStr( 24, 47, "ABY ZAPISAC DATE");
            u8g.drawStr( 12, 55, "I GODZINE PRZYTRZYMAJ");
            u8g.drawStr( 4, 63, "PRZYCISK PRZEZ 3 SEKUNDY");
          }
          else
          {
            u8g.drawStr( 5, 53, "ZAPISONO DATE I GODZINE!");
          }


        } while ( u8g.nextPage() );


        if (buttonState == LOW && counter2 > 5)
        {
          delay(1300);
          if (buttonState == LOW)
          {
            rtc.setTime(godziny, minuty, sekundy);
            rtc.setDate(dzien, miesiac, rok);
            zapisane = 1;
          }
        }

      }
      break;
    case 5:
      u8g.firstPage();
      do {
        u8g.setFont(u8g_font_5x8r);
        u8g.drawStr( 12, 6, "WYBOR LOGO PRODUCENTA");
        u8g.drawStr( 28, 25, "Logo:");
        switch (logo)
        {
          case 0:
            u8g.drawStr( 60, 25, "Honda");
            break;
          case 1:
            u8g.drawStr( 60, 25, "Kawasaki");
            break;
          case 2:
            u8g.drawStr( 60, 25, "Suzuki");
            break;
          case 3:
            u8g.drawStr( 60, 25, "BMW");
            break;
        }

        if (zapisane == 0)
        {
          u8g.drawStr( 14, 47, "KLINIECIE W PRZYCISK");
          u8g.drawStr( 10, 55, "ZMIENIA LOGO. ODCZEKAJ");
          u8g.drawStr( 10, 63, "10 SEKUND, ABY ZAPISAC");
        }
        else
        {
          u8g.drawStr( 30, 53, "ZAPISONO LOGO!");
        }

      } while (u8g.nextPage() );

      if (buttonState == LOW && counter2 > 5)
      {
        zapisane = 0;
        logo++;
        countermenu = 0;
        counter2 = 0;
        if (logo == 4)
        {
          logo = 0;
        }
      }

      countermenu++;
      if (countermenu > 90)
      {
        zapisane = 1;
        EEPROM.updateInt(64, logo);
      }

      break;
  }


}
